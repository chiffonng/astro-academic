    "rehype-katex": "^7.0.1",
    "rehype-pretty-code": "^0.14.1",
    "remark-emoji": "^5.0.1",
    "remark-math": "^6.0.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
