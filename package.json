  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "oxlint": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
