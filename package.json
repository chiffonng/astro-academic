{
  "name": "astro-academic",
  "type": "module",
  "version": "1.6.2",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
